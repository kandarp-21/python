    "#sum of natural numbers\n",
    "num=(int(input(\"enter the number:\")))\n",
    "if num < 0:\n",
    "  print(\"enter a positive number\")\n",
    "else:\n",
    "    sum=0\n",
    "    while(num>0):\n",
    "        sum += num\n",
    "        num -= 1\n",
    "    print(\"The sum is:\",sum)"
